<?xml version="1.0" encoding="UTF-8"?>
<message_set>
  <message>
    <name>ACOUSTIC_MOOS_POKE</name>
    <trigger>publish</trigger>
    <trigger_moos_var>ACOUSTIC_MOOS_POKE</trigger_moos_var>
    <size>32</size>
    <header>
      <id>50</id>
      <dest_id>
        <name>destination</name>
      </dest_id>
    </header>
    <layout>
      <string>
        <name>moos_variable</name>
        <max_length>13</max_length>
      </string>      
      <string>
        <name>value_to_poke</name>
        <max_length>13</max_length>
      </string>
    </layout>
    <on_receipt>
      <publish>
        <moos_var>%1%</moos_var>
        <format>%2%</format>
        <message_var algorithm="to_upper">moos_variable</message_var>
        <message_var>value_to_poke</message_var>
      </publish>
    </on_receipt>
    <queuing>
      <ttl>300</ttl>
      <value_base>10</value_base>
      <ack>true</ack>
    </queuing>
  </message>
</message_set>
